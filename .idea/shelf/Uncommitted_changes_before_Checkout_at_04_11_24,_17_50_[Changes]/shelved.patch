+import datetime
+
+
+def check_month(month):
+    if month < 1 or month > 12:
+        raise ValueError("Error! Month must be between 1 and 12.")
+
+
+def check_day(day, month, year):
+    days_in_month = {
+        1: 31,  # January
+        2: 28,  # February (29 in leap years)
+        3: 31,  # March
+        4: 30,  # April
+        5: 31,  # May
+        6: 30,  # June
+        7: 31,  # July
+        8: 31,  # August
+        9: 30,  # September
+        10: 31,  # October
+        11: 30,  # November
+        12: 31  # December
+    }
+
+    # Handle February for leap years
+    if month == 2 and is_leap_year(year):
+        max_day = 29
+    else:
+        max_day = days_in_month[month]
+
+    if day < 1 or day > max_day:
+        raise ValueError(f"Error! Day must be between 1 and {max_day} for month {month}.")
+
+
+def which_day(day, month, year):
+    date_obj = datetime.date(year, month, day)
+    return date_obj.strftime('%A')
+
+
+def is_leap_year(year):
+    return (year % 4 == 0 and year % 100 != 0) or (year % 400 == 0)
+
+
+def safe_which_day(day, month, year):
+    try:
+        check_month(month)
+        check_day(day, month, year)
+        return which_day(day, month, year)
+    except ValueError as e:
+        return str(e)
+
+
+def main():
+    while True:
+        try:
+            day = int(input("Enter day: "))
+            month = int(input("Enter month: "))
+            year = int(input("Enter year: "))
+            print(safe_which_day(day, month, year))
+            break
+        except ValueError:
+            print("Invalid input. Please enter a valid number for day, month, and year.")
+
+
+if __name__ == "__main__":
+    main()
